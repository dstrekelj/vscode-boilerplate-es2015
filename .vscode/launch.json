{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}\\src\\index.js",
            "stopOnEntry": false,
            "cwd": "${workspaceRoot}\\src\\",
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "console": "internalConsole",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}\\debug\\**\\*.js"
            ]
        }
    ]
}
